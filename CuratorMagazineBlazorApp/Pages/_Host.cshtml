@page "/"
@namespace CuratorMagazineBlazorApp.Pages
@using CuratorMagazineBlazorApp.Shared
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="../css/MainPage.css">
    <title>Main page</title>
    <base href="~/" />
</head>
<body>
    <div class="container">
        <component type="typeof(MainHeader)" render-mode="ServerPrerendered" />
        <component type="typeof(ModalWindowEnter)" render-mode="ServerPrerendered" />
        <component type="typeof(NavBar)" render-mode="ServerPrerendered" />
        <component type="typeof(RoleNavBar)" render-mode="ServerPrerendered" />
        <component type="typeof(Carousel)" render-mode="ServerPrerendered" />
        <component type="typeof(MainBody)" render-mode="ServerPrerendered" />
    </div>
    <component type="typeof(Footer)" render-mode="ServerPrerendered" />

    <script src="_framework/blazor.server.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</body>
</html>

